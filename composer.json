{
  "name": "jepsonwu/lib",
  "description": "Component library",
  "keywords": [
    "lib",
    "jepson"
  ],
  "homepage": "https://github.com/jepsonwu/lib",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "jepsonwu",
      "email": "wjp13671142513@163.com"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "joshcam/mysqli-database-class": ">=v2.6",
    "monolog/monolog": "^1.21"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5",
    "jakub-onderka/php-parallel-lint": "0.9"
  },
  "autoload": {
    "psr-4": {
      "Jepsonwu\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jepsonwu\\": "tests"
    }
  },
  "scripts": {
    "test": [
      "parallel-lint . --exclude vendor",
      "phpunit"
    ]
  }
}